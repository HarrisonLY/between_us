$('#gmaps_directions').html('');

$('#gmaps').html('<%= escape_javascript( gmaps( @gmap_directions_user_car ) ) %>');


Gmaps.map = new Gmaps4RailsGoogle();
Gmaps.load_map = function() {
    // Gmaps.map.map_options.maxZoom = 15;
    Gmaps.map.initialize();
      Gmaps.map.kml = <%= escape_javascript( @gmap_directions_user_car ) %>;
      Gmaps.map.create_kml();
    Gmaps.map.adjustMapToBounds();
    Gmaps.map.callback();
};

Gmaps.loadMaps();
